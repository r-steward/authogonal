{
  "name": "authogonal",
  "version": "1.0.0-pre",
  "description": "Typescript Authentication",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "verify": "npm run format && npm run lint && npm run build && npm run test",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-steward/authogonal.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r-steward/authogonal/issues"
  },
  "homepage": "https://github.com/r-steward/authogonal#readme",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.149",
    "create-ts-index": "^1.12.0",
    "jest": "^29.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "lodash": "^4.17.15",
    "logging-facade": "^1.0.0",
    "moment": "^2.29.1"
  }
}